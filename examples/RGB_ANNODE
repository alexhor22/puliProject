//this uses pwm 
int redPin = D3; 
int bluePin = D1; 
int greenPin= D2;
void setup() {
  pinMode(redPin, OUTPUT);
  pinMode(bluePin, OUTPUT);
  pinMode(greenPin, OUTPUT);
}
void loop() {
 
    digitalWrite(D3, LOW); //RED
    digitalWrite(D1, HIGH); //
    digitalWrite(D2, LOW);
}
